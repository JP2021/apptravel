{
  "name": "travel",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prestart": "node -e \"try{require.resolve('expo')}catch(e){console.error('Dependencias nao instaladas. Rode: npm install');process.exit(1)}\"",
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "api": "node pdf-api/server.js",
    "dev": "concurrently -n api,expo -c blue,green \"npm run api\" \"npm run start\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^3.0.1",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/material-top-tabs": "^7.4.13",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "cors": "^2.8.5",
    "expo": "~54.0.33",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-status-bar": "~3.0.9",
    "express": "^4.21.0",
    "pdf-parse": "^1.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-pager-view": "6.9.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
